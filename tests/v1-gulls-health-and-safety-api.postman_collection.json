							"    pm.expect(jsonData.License).to.have.property('periodFrom');\r",
							"    pm.expect(jsonData.License).to.have.property('periodTo');\r",
							"    pm.expect(jsonData.License).to.have.property('PermittedSpecies');\r",
							"\r",
							"    pm.expect(jsonData.License.PermittedSpecies).to.have.property('HerringGullId');\r",
							"    pm.expect(jsonData.License.PermittedSpecies).to.have.property('BlackHeadedGullId');\r",
							"    pm.expect(jsonData.License.PermittedSpecies).to.have.property('CommonGullId');\r",
							"    pm.expect(jsonData.License.PermittedSpecies).to.have.property('GreatBlackBackedGullId');\r",
